.login-button{
	text-align: center;
	margin-bottom: 68rpx;
}
.login-button ._button{
	width: 608rpx;
	height: 100rpx;
	border-radius: 60rpx;
	font-size: 30rpx;
	color: #FFFFFF;
	background: -webkit-gradient(linear,left top, right top,from(#D32F58),to(#F54570));
	background: -o-linear-gradient(left,#D32F58,#F54570);
	background: linear-gradient(to right,#D32F58,#F54570);
